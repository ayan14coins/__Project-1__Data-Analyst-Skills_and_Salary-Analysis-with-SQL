SELECT
    job_id,
    job_title,
    salary_year_avg,
    name as company_name,
    job_location,
    job_schedule_type
FROM
    job_postings_fact
LEFT JOIN
    company_dim on job_postings_fact.company_id = company_dim.company_id
WHERE
    salary_year_avg is NOT NULL
    -- AND job_location = 'Anywhere'
    AND job_title_short = 'Data Analyst'
ORDER BY
    salary_year_avg DESC
LIMIT
    10
